{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "commit": "git-cz",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "js-async-utils",
  "author": "Zainuddin99",
  "module": "dist/promise-wrapper.esm.js",
  "size-limit": [
    {
      "path": "dist/promise-wrapper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/promise-wrapper.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "semantic-release": "^19.0.3",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "Utilities for asynchronous tasks and functions",
  "bugs": {
    "url": "https://github.com/Zainuddin99/js-async-utils/issues"
  },
  "homepage": "https://github.com/Zainuddin99/js-async-utils#readme",
  "directories": {
    "example": "example"
  },
  "dependencies": {}
}

